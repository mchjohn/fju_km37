{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [ "./src/*"],
      "@utils/*": [ "./src/utils/*"],
      "@hooks/*": [ "./src/hooks/*"],
      "@styles/*": [ "./src/styles/*"],
      "@routes/*": [ "./src/routes/*"],
      "@assets/*": [ "./src/assets/*"],
      "@contexts/*": [ "src/contexts/*"],
      "@screens/*": [ "./src/screens/*"],
      "@services/*": [ "./src/services/*"],
      "@constants/*": [ "./src/constants/*"],
      "@interfaces/*": [ "./src/interfaces/*"],
      "@components/*": [ "./src/components/*"],
    }
  }
}
